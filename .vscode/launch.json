    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Tunnel",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/tunnel/tunnel.go",
            "env": {},
            "args": ["-config", "${workspaceFolder}/build/pkg/darwin_amd64/client_cfg/tunnel.yml", "-id", "id-test", "start-all"]
        },
        {
            "name": "Launch Tunnel-D",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/tunneld/tunneld.go",
            "env": {
                "CERT": "${workspaceFolder}/build/pkg/darwin_amd64/server_cfg/server.crt",
                "KEY": "${workspaceFolder}/build/pkg/darwin_amd64/server_cfg/server.key",
                "TUNNELADDR": "localhost:5223",
                "HTTPADDR": "localhost:4445",
                "HTTPSADDR": "localhost:4445"
            },
            "args": ["-tlsCrt", "${workspaceFolder}/build/pkg/darwin_amd64/server_cfg/server.crt", "-tlsKey", "${workspaceFolder}/build/pkg/darwin_amd64/server_cfg/server.key", "-httpsAddr", "", "-httpAddr", "", "-tunnelAddr", "localhost:5223"]
        }
    ]
}